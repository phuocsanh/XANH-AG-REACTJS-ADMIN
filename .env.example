# Environment Variables Template
# Copy this file to .env.local for development or .env.production for production

# API Configuration
VITE_API_URL=http://localhost:3003

# Gemini AI Model
VITE_GEMINI_MODEL=gemini-2.5-flash

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id
VITE_FIREBASE_VAPID_KEY=your_vapid_key

# Weather API Keys (Ưu tiên lấy từ Firebase Remote Config)
# Nhóm: API_KEY_TOMORROW_IO | Parameters: TOMORROW_API_KEY_1, TOMORROW_API_KEY_2, TOMORROW_API_KEY_3
# Fallback local (chỉ dùng nếu Remote Config trống)
# VITE_TOMORROW_API_KEY=key1,key2
